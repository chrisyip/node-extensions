{
  "rules": {
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "radix": 2,
    "no-eq-null": 0,
    "quotes": 0,
    "no-else-return": 1,
    "block-scoped-var": 2,
    "no-lonely-if": 1,
    "comma-style": [
      2,
      "last"
    ],
    "no-nested-ternary": 1,
    "curly": 2,
    "camelcase": 0,
    "no-undefined": 0,
    "no-multi-spaces": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "generator-star": [
      2,
      "start"
    ],
    "no-div-regex": 1,
    "strict": 0,
    "space-after-keywords": [
      2,
      "always"
    ],
    "no-reserved-keys": 2,
    "no-process-exit": 0,
    "no-new-require": 2,
    "no-mixed-requires": 2,
    "no-debugger": 1,
    "no-floating-decimal": 2,
    "no-loop-func": 1,
    "space-before-blocks": [
      2,
      "always"
    ],
    "no-trailing-spaces": 2,
    "no-wrap-func": 1,
    "space-before-function-parentheses": [
      2,
      "always"
    ],
    "no-extra-parens": 2,
    "no-path-concat": 2,
    "semi": [
      2,
      "never"
    ],
    "global-strict": 0,
    "new-cap": 0,
    "no-catch-shadow": 2,
    "handle-callback-err": 0,
    "no-mixed-spaces-and-tabs": [
      2,
      "smart-tabs"
    ],
    "eqeqeq": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "no-extend-native": 0
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
